;; A TPN with constraints

(defpclass parallel-feasible []
           :meta {:doc "A feasible choice to be chewed by contraint solver"}
           :methods [(defpmethod main
                                 {:doc "Entry point"}
                                 []
                                 (parallel :bounds [50 100]
                                           (option :bounds [55 65])
                                           (option :bounds [65 75])
                                           (option :bounds [75 85])))
                     (defpmethod option [])
                     ])
